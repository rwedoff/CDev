#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "list.h"


void init(slist_t *list){
	list_item_t *head = malloc(sizeof(list_item_t));
	list_item_t *tail = malloc(sizeof(list_item_t));
	head->prev = NULL;
	head->next=tail;
	tail->prev= head;
	tail->next= NULL;
	head->value = "";
	tail->value = "";
	list->head = head;
	list->tail = tail;


}

void print(slist_t *list){
	list_item_t * currTail = list->tail;
	list_item_t * currHead = list->head;

	if(currHead->next != NULL){
		while(currTail->next != NULL){
			printf(currTail->value);
			currTail = currTail->next;
		}
	}
}

int main(){


	slist_t * list = malloc(sizeof(slist_t));
	void init();
	//print(list);
	return 0;
}
