#ifndef LINKEDLIST_H_
#define LINKEDLIST_H_

typedef struct node{
        int value;
        struct node * next;
    }llNode;


void printList(llNode * head);
llNode push(llNode * head,int val);
llNode * createNode(int val);

#endif /* LINKEDLIST_H_ */
