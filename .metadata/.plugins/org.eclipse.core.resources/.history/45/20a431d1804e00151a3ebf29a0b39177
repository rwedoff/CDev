#include <stdio.h>
#include <stdlib.h>
#include "LinkedList.h"

void printList(llNode * head){
    llNode * current = head;
    while (current != NULL) {
        printf("%d\n", current->value);
        current = current->next;
    }
}

llNode push(llNode * head, int val){
    llNode * curr = head;
    printf("%d\n",curr->value);
    llNode newNode = createNode(val);
     newNode->next = curr;
    return  newNode;
}

llNode createNode(int val){
	llNode * newNode = malloc(sizeof(llNode));
	newNode->value = val;
	return * newNode;
}





